Number	Boiler_Thermo	"Boiler Status [MAP(thermostatmode.map):%d]"	<fire>	(GF_Hall)	{zwave="33:command=thermostat_mode,refresh_interval=600"}

/* Wake on LAN */
Switch	MainPC_Wake	"Main Computer Wake"	<computer>	(FF_Study)	{wol="10.1.1.0#74:d4:35:54:85:89"}

/* Network Pings */
Switch	Television_Alive	"TV On? [%s]"	<video>	(GF_Lounge)	{exec="<[/bin/sh@@-c@@ping -c 1 10.1.0.8 | grep \"packets transmitted\" | sed -e \"s/.*1 received.*/ON/\" -e \"s/.*0 received.*/OFF/\":60000:REGEX((.*))]"}
Switch	MainPC_Alive	"Main PC On? [%s]"	<computer>	(FF_Study)	{exec="<[/bin/sh@@-c@@ping -c 1 10.1.0.10 | grep \"packets transmitted\" | sed -e \"s/.*1 received.*/ON/\" -e \"s/.*0 received.*/OFF/\":60000:REGEX((.*))]"}
Switch	RichPhone_Alive "Richard Phone On? [%s]" <computer> (Mobile) {exec="<[/bin/sh@@-c@@ping -c 1 10.1.0.11 | grep \"packets transmitted\" | sed -e \"s/.*1 received.*/ON/\" -e \"s/.*0 received.*/OFF/\":60000:REGEX((.*))]"}

