/* Network Time NTP */
DateTime	Date	"Date [%1$tA, %1$td.%1$tm.%1$tY %1$tT]"	<calendar>	{ntp="Europe/London:en_GB"}

/* Hallway Aeon Labs Multisensor - Battery Powered - set Configuration item 101 to 225 (send all values) and 111 to 3600 (hourly) using Habmin */
Number	Temp_Sensor_Hall	"Hallway temperature: [%.1f C]"	<temperature> (GF_Hall,Hall_Temperature_Chart)	{zwave="34:command=sensor_multilevel,sensor_type=1"}
Number	Humid_Sensor_Hall	"Hall Relative humidity: [%.0f %%]"	<water>	(GF_Hall) {zwave="34:command=sensor_multilevel,sensor_type=5"}
Number	Lumin_Sensor_Hall	"Hall Luminance: [%.0f Lux]"	<sun>	(GF_Hall)	{zwave="34:command=sensor_multilevel,sensor_type=3"}
Number	Battery_Sensor_Hall	"Hall Sensor Battery: [%d %%]"	<battery> (GF_Hall,Battery)	{zwave="34:command=battery"}
Contact	Motion_Sensor_Hall	"Hall Sensor [MAP(motion.map):%s]"	(GF_Hall)	{zwave="34:command=sensor_binary,respond_to_basic=true"}

/* Landing Aeon Labs Multisensor - USB Powered */
